#include <iostream>
using namespace std;
int highd(int num)
{
    int i;
    for(i=num-1;i>=1;--i)
    {
        if(num%i==0)
        {
            return i;
        }
    }
    return 0;
}
int main()
{
    int num;
    printf("Enter a number: ");
    scanf("%d",&num);
    printf("Highest divisor of %d is %d", num, highd(num));
}
